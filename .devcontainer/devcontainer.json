{
	"name": "TROLIE Conformance Suite",
	"dockerComposeFile": "docker-compose.yml",
	"service": "jupyter",
	"workspaceFolder": "/workspace",
	"forwardPorts": [
		8888,
		5432,
		4010
	],
	"shutdownAction": "stopCompose",
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.python",
				"ms-python.vscode-pylance",
				"ms-toolsai.jupyter"
			]
		}
	},
	"onCreateCommand": "curl -o /workspace/openapi.yaml https://trolie.energy/openapi.yaml",
	"postCreateCommand": "pip install -r /workspace/requirements.txt && git config --global --add safe.directory /workspace",
	"containerEnv": {
		"NODE_EXTRA_CA_CERTS": "/workspace/.devcontainer/root.pem",
		"GIT_SSL_CAINFO": "/workspace/.devcontainer/root.pem"
	}
}